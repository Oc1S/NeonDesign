{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react",
    "declaration": true,
    "declarationDir": "lib",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true //不添加history报错
  },
  "include": ["src", "typings.d.ts", "svg.d.ts"],
  "exclude": ["node_modules", "src/**/demo", "src/**/__tests__"]
}
